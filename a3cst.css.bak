.logo{
	background-color:blue;
}
.navigation{
	display:flex;
	align-items:center;
	padding-top:10px;
	padding-left:50px;
	padding-right:30px;
	padding-bottom:10px;
	/*padding-top:50px;*/
	position:fixed;
	top:0; right:0; left:0;
	background:rgb(00,00,55,0.6);
	border-bottom-left-radius:20px;
	border-bottom-right-radius:20px;
	border-bottom-right-radius:20px;
	justify-content:space-between;
	z-index:1000;
	background-color:rgb(00,00,55);

	
}
nav{
	flex:1;
	text-align:right;
}
nav ul{
	display:inline-block;
	list-style:none;
	margin:0;
	padding:0;
}
nav ul li{
	display:inline-block ;
	margin-right:1px;

}

nav ul li a{
	text-decoration:none;
	font-size:15px;
	color:white;
	font-family:sans-serif;
	padding: 14px 16px;
	border-radius:4px;
}
nav ul li button{
	font-size:20px;
	color:white;
	outline:none;
	border:none;
	background:transparent;
	cursor:pointer;
	font-family:sans-serif;
	border-radius:4px;
}
nav ul li button:hover
{
	color:aqua;
	border-radius:4px;
}
nav ul li a:hover 
{
	color:aqua;
	background:#000;
	border-radius:4px;
	
} 
.menu ul li :hover{
	border-radius:4px;
	
}

